{
  "name": "Aspire",
  "description": "Be the person you wish you were",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.1.1",
  "offline_enabled": true,
  "icons": {
    "36": "assets/icons/icon.png",
    "48": "assets/icons/icon.png",
    "78": "assets/icons/icon.png",
    "96": "assets/icons/icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "notifications"
  ]
}
